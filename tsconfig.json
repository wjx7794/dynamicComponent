{
  "compilerOptions": {
    "strict": true,
    "declaration": true,
    "skipLibCheck": true,
    "esModuleInterop": true,
    "jsx": "react",
    "baseUrl": "./",
    "paths": {
      "@@/*": [".dumi/tmp/*"],
      "dynamicComponent": ["src"],
      "dynamicComponent/*": ["src/*", "*"]
    },
    "allowImportingTsExtensions": true,
    "noEmit": true
  },
  "include": [".dumirc.ts", "src/**/*"]
}
